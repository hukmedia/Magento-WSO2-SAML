<?xml version="1.0"?>
<config>
    <modules>
        <Hukmedia_Wso2>
            <version>0.9.1</version>
        </Hukmedia_Wso2>
    </modules>
    <global>
        <models>
            <hukmedia_wso2>
                <class>Hukmedia_Wso2_Model</class>
                <resourceModel>hukmedia_wso2_resource</resourceModel>
            </hukmedia_wso2>
            <hukmedia_wso2_resource>
                <class>Hukmedia_Wso2_Model_Resource</class>
                <entities>
                    <sessionindex>
                        <table>sessionindex</table>
                    </sessionindex>
                </entities>
            </hukmedia_wso2_resource>
        </models>
        <resources>
            <hukmedia_wso2_setup>
                <setup>
                    <module>Hukmedia_Wso2</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </hukmedia_wso2_setup>
            <hukmedia_wso2_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </hukmedia_wso2_read>
            <hukmedia_wso2_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </hukmedia_wso2_write>
        </resources>
        <blocks>
            <hukmedia_wso2>
                <class>Hukmedia_Wso2_Block</class>
            </hukmedia_wso2>
        </blocks>
        <helpers>
            <hukmedia_wso2>
                <class>Hukmedia_Wso2_Helper</class>
            </hukmedia_wso2>
        </helpers>
    </global>
    <frontend>
        <routers>
            <hukmedia_wso2>
                <use>standard</use>
                <args>
                    <module>Hukmedia_Wso2</module>
                    <frontName>wso2</frontName>
                </args>
            </hukmedia_wso2>

            <customer>
                <args>
                    <modules>
                        <hukmedia_wso2 before="Mage_Customer">Hukmedia_Wso2</hukmedia_wso2>
                    </modules>
                </args>
            </customer>

        </routers>
    </frontend>
    <adminhtml>
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <hukmedia_wso2_general>
                                            <title>WSO2 Identity Server general settings</title>
                                        </hukmedia_wso2_general>
                                        <hukmedia_wso2_saml>
                                            <title>WSO2 Identity Server SAML settings</title>
                                        </hukmedia_wso2_saml>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
